{
  "importOrder": [
    "^(react|react-dom)$",
    "^([a-z]|@[a-z])",
    "",
    ".*"
  ],
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrderSortSpecifiers": true,
  "arrowParens": "always",
  "semi": true,
  "trailingComma": "none",
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "printWidth": 120,
  "jsxSingleQuote": true
}